{
    "version":2,
    "builds": [
      {
        "src": "api/leaderboard/wsgi.py",
        "use": "@vercel/python",
        "config":{"runtime":"python3.10.7"}
      },
      {
        "src":"build_files.sh",
        "use":"@vercel/python",
        "config":{
            "distDir":"staticfiles_build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/leaderboard/wsgi.py"
      }
    ]
}